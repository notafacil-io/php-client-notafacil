    "require": {
        "php": "^7.2.5"
    },
 
    "require-dev": {
        "symfony/var-dumper": "^5.2",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "NotaFacil\\Common\\":"src/NotaFacil"
        },
        "files": ["src/NotaFacil/helper/helperNotafacil.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "NotaFacil\\Common\\Test\\":"src/Test"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
